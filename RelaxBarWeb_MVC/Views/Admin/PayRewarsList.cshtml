@model List<RelexBarBLL.Models.AdminPayListModel>
@for (int i = 0; i < Model.Count; i++)
{
    <tr>
        <th>@Model[i].Phone</th>
        <th>@(Model[i].InOut == 1 ? "+" : "-")@Model[i].Val</th>
        <th>@(Model[i].PayType == 1 ? "余额" : "筹码")</th>
        <th>@(Model[i].FromTo == 13 ? "佣金" : "红包")</th>
        <th>@Model[i].Remark</th>
        <th>@Model[i].CreateTime</th>
    </tr>
}
<script>createFooter(@ViewData["sum"])</script>
